    {
        "id": "19336663eb434a01",
        "type": "function",
        "z": "323bc1f107eef839",
        "name": "EventosPQA",
        "func": "var Lectura = flow.get(\"LecturaPQA\", \"memoryOnly\") || 0;\ncontext.set(\"Lectura\", Lectura, \"memoryOnly\");\n\n\n\nif (Lectura == \"LecturaPQA\") {\n    //Adquisicion de los datos de un evento\n    var Uint8 = new Uint8Array(6);\n    var NewestEvent = msg.payload[0];\n    var StartingGroup = msg.payload[1];\n    var Timestamp1 = msg.payload[3];\n    var Timestamp2 = msg.payload[4];\n    var Timestamp3 = msg.payload[5];\n    var Timestamp4 = msg.payload[6];\n    var VSagVSwell = msg.payload[7];\n    var RatedValue = msg.payload[8];\n    var Threshold = msg.payload[9];\n    var HalfCycle = msg.payload[10];\n    //Corrimiento de datos\n    Uint8[0] = Timestamp1;\n    Uint8[1] = Timestamp1 >> 8;\n    Uint8[2] = Timestamp2;\n    Uint8[3] = Timestamp2 >> 8;\n    Uint8[4] = Timestamp3;\n    Uint8[5] = Timestamp3 >> 8;\n    var year = Uint8[1] + 2000;\n\n    var dataEvento = { //Data del evento consultado  estructura\n        \"NewestEvent\": NewestEvent,\n        \"StartingGroup\": StartingGroup,\n        \"Fecha\": year + \"/\" + Uint8[0] + \"/\" + Uint8[3],\n        \"Hora\": Uint8[2] + \":\" + Uint8[5] + \":\" + Uint8[4],\n        \"Milisegundos\": Timestamp4,\n        \"VSagVSwell\": VSagVSwell,\n        \"RatedValue\": RatedValue,\n        \"Threshold\": Threshold,\n        \"HalfCycle\": HalfCycle\n    };\n    context.set(\"dataEvento\",dataEvento,\"memoryOnly\");\n    context.set(\"posActual\", dataEvento.StartingGroup,\"memoryOnly\");\n\n\n    //Guardar los eventos que llegan \n    var EventosPQA = flow.get(\"EventosPQA\") || []; //Arreglo para guardar los eventos\n    flow.set(\"NewestEvent\", NewestEvent, \"memoryOnly\"); //Guarda el numero del ultimo evento\n    var flag = 0;\n    var tamano = EventosPQA.length;\n    context.set(\"tamano\",tamano,\"memoryOnly\");\n    context.set(\"NewestEvent\", NewestEvent,\"memoryOnly\");\n    for (let j = 1; j <= NewestEvent; j++) {\n        node.status({fill:\"red\",shape:\"ring\",text:\"ingreso\"});\n        if (typeof EventosPQA[j].StartingGroup != 'undefined'){\n            if (dataEvento.StartingGroup == EventosPQA[j].StartingGroup) {\n                flag = 0;\n                break;\n            } else {\n                flag = 1;\n            }\n        }else{\n            //inicializacion\n            flag = 1;\n        }\n    }\n    context.set(\"flag\",flag,\"memoryOnly\");\n    if (flag == 1) {\n        EventosPQA.push(dataEvento);\n    }\n    flow.set(\"EventosPQA\", EventosPQA); //Guardar el evennto en el array de eventos \n    return msg;\n\n} else {\n    node.status({ fill: \"red\", shape: \"ring\", text: \"No tiene nada\" });\n}",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 630,
        "y": 260,
        "wires": [
            []
        ]
    },
